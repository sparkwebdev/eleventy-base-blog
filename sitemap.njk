---
title: Sitemap
pagination:
  data: collections.all
  size: 20
  alias: entries
layout: layouts/home.njk
permalink: /sitemap/{% if pagination.pageNumber > 0 %}{{ pagination.pageNumber }}/{% endif %}
---
<ul>
{%- for entry in entries %}
  <li><a href="{{ entry.url }}">{{ entry.data.title or entry.data.renderData.title or 'Home' }}</a></li>
{%- endfor %}
</ul>
